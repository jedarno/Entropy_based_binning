import numpy as np

def entropy(X):
  labels, counts = np.unique(X[:,1], return_counts = 1)
  num_of_labels = labels.size
  entropy = 0

  for i in range(num_of_labels):
    p = counts[i]/X.shape[0]
    print(counts[i], X.shape)
    print(p)
    print(p * np.log2(p))
    entropy -= p * np.log2(p)
    print(entropy)
  return entropy
